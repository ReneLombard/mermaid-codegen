{
  "language": "CSharp",
  "extension": "cs",
  "namespace": {
    "prefixToIgnore" :"HelloWorld"
  },
  "mappings": {
    "Scope": {
      "Public": "public",
      "Private": "private",
      "Protected": "protected"
    },
    "Type": {
      "Number": "int",
      "String": "string",
      "REGEX:~(.*)~": "<$1>"
    }
  }
}