{
    "language": "CSharp",
    "extension": "cs",
    "namespace": {
        "prefixToIgnore": "Company.VTC"
    },
    "mappings": {
        "Scope": {
            "Public": "public",
            "Private": "private",
            "Protected": "protected"
        },
        "Type": {
            "Number": "int",
            "String": "string",
            "REGEX:~(.*)~": "<$1>"
        }
    }
}
